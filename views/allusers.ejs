<link rel="stylesheet" href="<%= assetPath('css/allusers.css') %>">
<h1>People You May Know</h1>

<div class="users_list">
    <% for(u of get_users){%>
        <div class="card">
            <%if(u.avatar){%>
                <img src="<%=u.avatar%>" alt="<%=u.name %>">
            <%}else{%>
                <img src="images/default_pic-51d5024eb0.png">
            <%}%>
            <div class="name">
                <a href="/users/profile/<%=u.id %>"><%=u.name%></a>
            </div>
        </div>
    <%}%>
</div>